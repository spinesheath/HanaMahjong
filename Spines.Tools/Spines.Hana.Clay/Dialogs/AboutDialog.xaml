<Window x:Class="Spines.MachiKae.Dialogs.AboutDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="About" Height="300" Width="300" x:ClassModifier="internal">
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Text="MachiKae"/>
        <TextBlock DockPanel.Dock="Top" Text="Tile images taken from:"/>
        <TextBlock DockPanel.Dock="Top">
            <Hyperlink Click="OnGoToTileImages">
                <TextBlock>http://suzume.hakata21.com/5zats/haiga.html</TextBlock>
            </Hyperlink>
        </TextBlock>
    </DockPanel>
</Window>
